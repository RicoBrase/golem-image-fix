{
  "manifest_version": 2,
 
  "name": "Golem Image Fix",
  "version": "0.1.5",
  "author": "Rico Brase",
  "description": "Stellt die 'Afterload'-Bilder auf Golem.de trotz ScriptBlocker dar.",
  
  "icons": {
    "128": "golem128.png",
	"48": "golem48.png",
	"16": "golem16.png"
  },
  
  "browser_action": {
    "default_icon": {
    "128": "golem128.png",
	"48": "golem48.png",
	"16": "golem16.png"
     },
	 "default_title": "Golem Image Fix",
	 "default_popup": "about.html"
  },
 
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["http://*.golem.de/*"]
  }],
  
  "homepage_url": "https://github.com/RicoBrase/golem-image-fix"
 
}